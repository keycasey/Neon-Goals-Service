  "search_path": "/cars",
  "note": "CarMax uses URL path structure for make and body type, with query params for other filters. Filters are loaded dynamically via button clicks.",
  "url_format": {
    "pattern": "/cars/{make}/{trim}/{model}?{params}",
    "example": "https://www.carmax.com/cars/gmc/denali-ultimate/sierra-3500?showreservedcars=false",
    "important_note": "CarMax uses a SPECIFIC format: /cars/{make}/{trim}/{model}. The TRIM comes BEFORE the model in the URL path. For trucks, this is crucial.",
    "path_components": {
      "make": "Vehicle make (lowercase: gmc, ford, chevrolet, etc.)",
      "trim": "Trim level with hyphens (denali-ultimate, at4, lariat, platinum, etc.) - THIS COMES BEFORE MODEL!",
      "model": "Model with series number (sierra-1500, sierra-2500, sierra-3500, f-150, f-250, etc.) - HD suffix is removed (3500 not 3500HD)"
    },
    "examples": {
      "gmc_sierra_1500_denali": "/cars/gmc/denali/sierra-1500",
      "gmc_sierra_2500_denali": "/cars/gmc/denali/sierra-2500",
      "gmc_sierra_3500_denali_ultimate": "/cars/gmc/denali-ultimate/sierra-3500",
      "ford_f_150_lariat": "/cars/ford/lariat/f-150",
      "ford_f_150_platinum": "/cars/ford/platinum/f-150"
    },
    "query_params": {
      "showreservedcars": "Show reserved cars (true/false)",
      "priceMax": "Maximum price",
      "priceMin": "Minimum price",
      "yearMax": "Maximum year",
      "yearMin": "Minimum year",
      "mileageMax": "Maximum mileage"
    }
  },
